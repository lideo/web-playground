{
  "name": "web-playground",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "devclient": "cd client && yarn watch",
    "client": "cd client && yarn build",
    "devserver": "NODE_ENV=development nodemon --ext js,njk,json ./bin/www",
    "server": "NODE_ENV=production node ./bin/www",
    "devstart": "concurrently \"yarn devserver\" \"yarn devclient\"",
    "start": "concurrently \"yarn server\" \"yarn client\"",
    "test": "NODE_ENV=test node_modules/mocha/bin/mocha ./test --timeout 10000",
    "lint:prettier": "prettier --check \"**/*.js\"",
    "fix:prettier": "prettier --write \"**/*.js\"",
    "lint:eslint": "eslint \"**/*.js\"",
    "fix:eslint": "eslint --fix \"**/*.js\""
  },
  "dependencies": {
    "apollo-server-express": "^2.14.2",
    "bcrypt": "^3.0.6",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-mongo": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.2.0",
    "he": "^1.2.0",
    "helmet": "^3.21.1",
    "http-errors": "~1.6.3",
    "mongoose": ">=5.7.5",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "async": "^3.1.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.1",
    "nodemon": "^1.19.3",
    "prettier": "1.18.2",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2"
  },
  "resolutions": {
    "minimist": "^1.2.3"
  }
}

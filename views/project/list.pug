extends ../layout

block content
    h1= title

    ul
        - projectList.sort(function(a, b) {let textA = a.name.toUpperCase(); let textB = b.name.toUpperCase(); return (textA < textB) ? -1 : (textA > textB) ? 1 : 0;});
        each project in projectList
            li
                a(href=project.url) #{project.name}
                |  (#{project.user.full_name})
        else
            li There are no books.